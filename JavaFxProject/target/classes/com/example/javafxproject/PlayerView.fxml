<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxproject.PlayerViewController">
    <VBox alignment="CENTER" spacing="20" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0">
        <TextArea fx:id="playerTextArea" editable="false" wrapText="true" styleClass="text-area" prefHeight="400.0" prefWidth="600.0" />
        <Button fx:id="showPlayersButton" text="Show Players" styleClass="button-primary" onAction="#displayPlayers" />
        <Button fx:id="backButton" text="Back to Main Menu" styleClass="button-secondary" />
    </VBox>
</AnchorPane>
